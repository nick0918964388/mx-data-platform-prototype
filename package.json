{
  "name": "mx-data-platform-prototype",
  "version": "1.0.0",
  "description": "高效資料分析平台 - High-Efficiency Data Analysis Platform",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
